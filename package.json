{
  "name": "react-vite-electron-desktop-screensaver",
  "private": true,
  "version": "0.0.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"electron .\" \"node api.js\"",
    "electron": "electron .",
    "start": "npm run build && npm run electron",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:electron": "electron-builder"
  },
  "build": {
    "appId": "thesunrightnow",
    "productName": "The Sun Right Now",
    "files": [
      "dist/**/*",
      "main.js",
      "preloader.js",
      "api.js"
    ],
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "portable"
    },
    "mac": null,
    "linux": null
  },
  "dependencies": {
    "cors": "^2.8.5",
    "electron-is-dev": "^3.0.1",
    "express": "^5.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
